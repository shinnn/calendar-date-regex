{
  "name": "calendar-date-regex",
  "version": "1.0.0",
  "description": "Regular expression for matching ISO 8601 calendar dates",
  "repository": "shinnn/calendar-date-regex",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/calendar-date-regex/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "regex",
    "regexp",
    "regular-expression",
    "date",
    "year",
    "month",
    "day",
    "calendar",
    "iso",
    "iso8601",
    "browser",
    "client-side"
  ],
  "devDependencies": {
    "eslint": "^0.13.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.10.0",
    "require-bower-files": "^2.0.0",
    "tap-spec": "^2.2.0",
    "tape": "^3.4.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
