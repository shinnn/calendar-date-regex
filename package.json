    "browser",
    "client-side"
  ],
  "devDependencies": {
    "@shinnn/eslintrc": "^1.0.0",
    "eslint": "^0.21.2",
    "istanbul": "^0.3.14",
    "istanbul-coveralls": "^1.0.2",
    "require-bower-files": "^2.0.0",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
